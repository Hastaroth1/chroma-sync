<Window x:Class="Ultrabox.ChromaSync.MainBrowser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ultrabox.ChromaSync"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="300">
    <Window.Background>
        <SolidColorBrush Color="{DynamicResource ChromaSyncBlack}"/>
    </Window.Background>
    <Grid>
        <DockPanel HorizontalAlignment="Left" Height="100" LastChildFill="False" Margin="60,75,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
