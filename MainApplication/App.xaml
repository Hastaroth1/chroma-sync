<Application x:Class="Ultrabox.ChromaSync.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Ultrabox.ChromaSync"

             Startup="App_Startup"
             Exit="Application_Exit">
    <Application.Resources>
        <Color x:Key="ChromaSyncPink">#FFFF3366</Color>
        <Color x:Key="ChromaSyncBlack">#FF141414</Color>
        <Color x:Key="ChromaSyncGrey">#FF3C3C3C</Color>
        <Color x:Key="ChromaSyncTranslucent">#3F3C3C3C</Color>
        <Color x:Key="ChromaSyncMedGrey">#FF666666</Color>

    </Application.Resources>
</Application>